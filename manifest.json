{
  "manifest_version": 2,
  "name": "StockExt",
  "description": "Stock monitoring",
  "version": "1.0.0",
  "icons": {
    "16": "stockIcon.png",
    "48": "stockIcon.png",
    "128": "stockIcon.png"
  },
  "browser_action": {
    "default_icon": "stockIcon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}